<%- include('partials/header') -%>
<div class="wbg-container">
  <div class="wbg">
    <img id="postImage" class="wbg-image" src="<%= post.image%>" alt="image of kairde (card) that grows in size on hover and focus/selection"/>
    <h2 id="h2Name"><%= post.title %></h2>
    <p class="shrinkMePlease"><b>ImportLink:</b> <%-post.link%></p>
    <button class="btn myRadio primary-text previewSwap box btnCorrecto lineBtn lineBtn2 unpress" type="button" onclick="forceDown(); this.blur();">Download</button>
  </div>
</div>

