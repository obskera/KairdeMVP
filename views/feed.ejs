<%- include('partials/header') -%>
   
    <% if (posts.length < 1) {%>
      <div class="container dynamic-container">
        <h1>My Kairdes</h1>
        <h2>No saved Kairdes, Make or upload a Kairde</h2>
        <!-- <h2>Nothing here yet, Upload something or make a Kairde.</h2> -->

      </div>
      <%} else {%>
    <div class="row justify-content-center mt-5 dynamic-container snapBorder feed1">
      <h1>My Kairdes</h1>
      <ul class="row list-unstyled phone-column">
        <% for(var i=0; i<posts.length; i++) {%>
          <li class="col-4 justify-content-center mt-5">
            <a href="/post/<%= posts[i]._id%>" class="">
              <img class="img-fluid shadow snapBorder boxG" src="<%= posts[i].image%>">
            </a>
            <h2><%=posts[i].title%></h2>
            <!-- REPLACE WITH PROPER URL -->
            <%if (posts[i].share) {%>
              <div class="box">
                <p class="shrinkMePlease"><b>ViewLink:</b> http://localhost:3000/kairde/<%-posts[i].link%></p>
                <p class="shrinkMePlease"><b>ImportLink:</b> <%-posts[i].link%></p>
              </div>
           
            <%}%>
          </li>
        <% } %>
        </ul>
    </div>
    <%}%>
    <div class="dynamic-container feed2" >
      <%- include('partials/upload')-%>
      <%- include('partials/cookieDisclaimer')%>
    </div>
   
   
 

 
<%- include('partials/footer') -%>